{"components/common/small-paginator":"<div class=\"pagination\">\n    <ul class=\"pagination-list pagination-list--small\">\n        {{#if previous}}\n            <li class=\"pagination-item pagination-item--previous\">\n                <a class=\"pagination-link\" href=\"{{previous}}\" data-faceted-search-facet>\n                    <i class=\"icon\" aria-hidden=\"true\">\n                        <svg>\n                            <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-left\"></use>\n                        </svg>\n                    </i>\n                    {{lang 'common.previous'}}\n                </a>\n            </li>\n        {{/if}}\n\n        {{#if this.links.length}}\n        {{#each this.links}}\n            <a class=\"pagination-link\" href=\"{{this.link}}\">\n                <li class=\"pagination-item\">{{this.number}}</li>\n            </a>\n        {{/each}}\n        {{/if}}\n\n        {{#if next}}\n            <li class=\"pagination-item pagination-item--next\">\n                <a class=\"pagination-link\" href=\"{{next}}\" data-faceted-search-facet>\n                    {{lang 'common.next'}}\n                    <i class=\"icon\" aria-hidden=\"true\">\n                        <svg>\n                            <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-right\"></use>\n                        </svg>\n                    </i>\n                </a>\n            </li>\n        {{/if}}\n    </ul>\n</div>\n\n"}
